&qupv3_se13_i2c_pins {
	oplus_se13_i2c_sda_active: oplus_se13_i2c_sda_active {
		mux {
			pins = "gpio59";
			function = "qup1_se6_l0";
		};

		config {
			pins = "gpio59";
			drive-strength = <2>;
			bias-disable;
		};
	};

	oplus_se13_i2c_scl_active: oplus_se13_i2c_scl_active {
		mux {
			pins = "gpio60";
			function = "qup1_se6_l1";
		};

		config {
			pins = "gpio60";
			drive-strength = <2>;
			bias-disable;
		};
	};

	qupv3_se13_i2c_pulldown: qupv3_se13_i2c_pulldown {
		mux {
			pins = "gpio59", "gpio60";
			function = "gpio";
		};

		config {
			pins = "gpio59", "gpio60";
			drive-strength = <2>;
			bias-pull-down;
			output-low;
		};
	};

	qupv3_se13_i2c_pullup: qupv3_se13_i2c_pullup {
		mux {
			pins = "gpio59", "gpio60";
			function = "gpio";
		};

		config {
			pins = "gpio59", "gpio60";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
	};
};

