                oplus_virtual_buck: oplus,virtual_buck {
                        compatible = "oplus,virtual_buck";
                        oplus,buck_ic = <&oplus_pmiv0108_charger>;
                        oplus,buck_ic_index = <0>;
                        oplus,buck_ic_connect = <OPLUS_CHG_IC_CONNECT_PARALLEL>;
                        oplus,main_charger = <0>;
